    //发票税额百分号
    function shuieSymbol(x) {
        var pattern = /^\d+%$|^\d+\.{1}\d+%$/;
        var pattern1 = /^\d+$|^\d+\.{1}\d+$/;
        var fapiaoshuie = $(x).closest('tr').find('.shuieSymbol')[0].value;
        var shuiekuang = $(x).closest('tr').find('.shuieSymbol')[0];
        var n = (fapiaoshuie.split('.')).length - 1;
        if (fapiaoshuie != "" && fapiaoshuie != null) {
            if (fapiaoshuie.indexOf(" ") == -1 && n <= 1) {
                if (pattern.test(fapiaoshuie)) {
                    fapiaoshuie = fapiaoshuie.substring(0, fapiaoshuie.length - 1);
                } else if (!pattern1.test(fapiaoshuie)) {
                    fapiaoshuie = "";
                    layer.msg("发票税额或税点输入不规范,请重新输入", {
                        icon: 5
                    });
                } else {
                    fapiaoshuie = fapiaoshuie + "%";
                }
            } else {
                fapiaoshuie = "";
                layer.msg("发票税额或税点输入不规范，请重新输入", {
                    icon: 5
                });
            }
            $(shuiekuang).val(fapiaoshuie);
        } else {
            fapiaoshuie = "";
            layer.msg("请输入发票税额或税点", {
                icon: 5
            });
        }
    }
